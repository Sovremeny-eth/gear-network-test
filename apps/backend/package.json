  "scripts": {
    "all": "run-p lint build",
    "all:clean": "run-s clean all",
    "build": "rollup -c",
    "build:clean": "run-s clean build",
    "ci:all": "run-p lint build",
    "clean": "shx rm -rf dist .build",
    "deps": "pnpm --filter \"{.}...\" build",
    "deps:clean": "pnpm --filter \"{.}...\" build:clean",
    "dev": "run-s dev:build dev:run",
    "dev:build": "swc src -d .build/dev",
    "dev:build:watch": "swc -w src -d .build/dev",
    "dev:run": "run-p dev:build:watch dev:watch",
    "dev:watch": "nodemon .build/dev/main.js",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,json,md}\"",
    "lint": "eslint .",
    "start": "node --enable-source-maps ./dist/gear-test.main.app.js"
  }
